[run]
source = src
data_file = tests/.coverage
omit = 
    */tests/*
    */__pycache__/*
    */migrations/*
    */alembic/*
    */venv/*
    */env/*
    */__init__.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

precision = 2
show_missing = True
skip_covered = False

[html]
directory = tests/htmlcov

[xml]
output = tests/coverage.xml

